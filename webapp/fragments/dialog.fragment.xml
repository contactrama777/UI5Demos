<core:FragmentDefinition  xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core"  xmlns="sap.m">
<Dialog  title="Dialog Example" type= "Message">
	<content>
		<VBox>
			<Text text="{j1>/name}"></Text>
			<Text text="Do you want to Approve?"></Text>
		</VBox>
	<HBox>
		<Button text="Approve" press="closeButton"></Button>
		<Button text="Reject" press="rejectButton"></Button>
		</HBox>
	</content>
	</Dialog>	
</core:FragmentDefinition>